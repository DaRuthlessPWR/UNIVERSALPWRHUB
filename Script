local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("U/A PWR HUB", "Synapse")

-- Games
local Main = Window:NewTab("Games")
local MainSection = Main:NewSection("Games")
 
 
MainSection:NewButton("Project Avatar", "Best Project avatar GUI", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/DaRuthlessPWR/PA-PWR-HUB/main/%7BPA%7D%20PWR%20HUB%20BETA%20Free%20(2).txt"))()
    end)

MainSection:NewButton("Prison Life Prevail Remake", "Best Prison Life GUI", function()
    loadstring('HASHPX,LOADPX = "0092foz62c3unz2exvc32e3ur75ez9x6",true')()
loadstring(game:HttpGet("https://raw.githubusercontent.com/DaRuthlessPWR/prevail-remake/main/prevv2.lua", true))()
end)


MainSection:NewKeybind("Toggle UI", "Toggle hub", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)

-- Tools
local ToolsTab = Window:NewTab("Tools")
local ToolsSection = ToolsTab:NewSection("Tools")

ToolsSection:NewButton("PWR RTX", "Best RTX can be laggy in high demand games", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/fMHgDVD7", true))()
end)
